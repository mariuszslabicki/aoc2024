    "group_a = []\n",
    "group_b = []\n",
    "\n",
    "with open('input') as f:\n",
    "    for line in f:\n",
    "        values = line.strip().split()\n",
    "        group_a.append(int(values[0]))\n",
    "        group_b.append(int(values[1]))\n",
    "\n",
    "score = 0\n",
    "\n",
    "for el in group_a:\n",
    "    score += el * group_b.count(el)\n",
    "\n",
    "print(score)"
   ]
  }
